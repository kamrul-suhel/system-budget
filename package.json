    "electron": "^2.0.1",
    "electron-packager": "^12.1.0",
    "jquery": "^3.2",
    "laravel-mix": "^2.0",
    "lodash": "^4.17.4",
    "vue": "^2.5.7",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.17",
    "vuex": "^3.0.1"
  }
